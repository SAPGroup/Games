<Application x:Class="WPFLauncher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundBrush">#FF35183E</SolidColorBrush>
        <SolidColorBrush x:Key="HomeBrush">#FFE82C8A</SolidColorBrush>
        <SolidColorBrush x:Key="GameBrush">#FF59B031</SolidColorBrush>
        <SolidColorBrush x:Key="ProfileBrush">#FF67C3CF</SolidColorBrush>
        <SolidColorBrush x:Key="FontBrush">#FFFFFFFF</SolidColorBrush>
        <SolidColorBrush x:Key="HoverBrush">#FFFFFF00</SolidColorBrush>

        
        <Color x:Key="BackgroundColor">#FF35183E</Color>
        <Color x:Key="HomeColor">#E82C8A</Color>
        <Color x:Key="GameColor">#59B031</Color>
        <Color x:Key="ProfileColor">#67C3CF</Color>
        <Color x:Key="FontColor">#FFFFFF</Color>
        <Color x:Key="HoverColor">#FFFF00</Color>
    </Application.Resources>
</Application>
